msgid "Artisan Studio"
msgstr "장인 작업실"

msgctxt "8c61b9b3-b145-419c-ab7c-df0b1d5d8c7d"
msgid "Tirval's Leatherwork Shop"
msgstr "터발(Tirval)의 가죽 상점"

msgctxt "92937f9c-6947-4da7-95e0-ada87b2e7ee2"
